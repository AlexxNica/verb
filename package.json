  "keywords": [
    "verb"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "assemble",
        "base",
        "generate"
      ]
    },
    "reflinks": [
      "assemble",
      "generate",
      "update",
      "verb",
      "verb-readme-generator",
      "handlebars",
      "lodash",
      "swig",
      "gulp"
    ],
    "lint": {
      "reflinks": true
    }
  },
  "lintDeps": {
    "ignore": []
  }
}
